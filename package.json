{
  "name": "github-project-helper",
  "displayName": "GitHub Project Helper",
  "description": "Manage GitHub Project directly from VS Code with GitHub CLI integration",
  "version": "2.0.1",
  "publisher": "MaquestiauxLabs",
  "author": {
    "name": "Jean-Philippe Maquestiaux",
    "email": "maquejp@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaquestiauxLabs/github-project-helper.git"
  },
  "homepage": "https://github.com/MaquestiauxLabs/github-project-helper#readme",
  "bugs": {
    "url": "https://github.com/MaquestiauxLabs/github-project-helper/issues"
  },
  "keywords": [
    "github",
    "project",
    "issues",
    "status",
    "v2",
    "workflow",
    "cli",
    "scm"
  ],
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.109.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "github-project-helper.openGitHubProjects",
        "title": "GitHub Projects: Open Project Helper"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "package-vsix": "vsce package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.39.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}